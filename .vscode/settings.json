{
  "files.watcherExclude": {
    "**/references/**": true,
    "**/node_modules/**": true,
    "**/.venv/**": true,
    "**/target/**": true
  },
  "search.exclude": {
    "**/node_modules/**": true,
    "**/bower_components/**": true,
    "**/*.code-search": true
  },
  "python.analysis.exclude": [
    "**/references/**"
  ],
  "typescript.validate.enable": false,
  "javascript.validate.enable": false,
  "java.compile.nullAnalysis.mode": "automatic"
}
